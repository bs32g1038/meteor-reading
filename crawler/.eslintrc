{
    "extends": [
        "eslint:recommended"
    ],
    "root": true,
    "rules": {
        "no-console": "off",
        "no-empty": 0,
        "no-const-assign": 2,
        "no-dupe-keys": 2,
        "no-redeclare": 2,
        "no-unreachable": 2,
        "no-use-before-define": 2,
        "use-isnan": 2,
        "no-undef": 1,
        "valid-typeof": 2,
        "no-var": 2,
        "no-func-assign": 2,
        "semi": [
            "error",
            "always"
        ],
        "jsx-quotes": 2,
        "no-multiple-empty-lines": 2,
        "no-unused-vars": [
            2,
            {
                "vars": "all",
                "args": "none"
            }
        ],
        "lines-around-comment": [
            1,
            {
                "beforeBlockComment": true
            }
        ],
        "indent": [
            "error",
            4,
            {
                "SwitchCase": 1
            }
        ]
    },
    "env": {
        "node": true,
        "es6": true,
        "browser": true
    },
    "parserOptions": {
        "ecmaVersion": 7,
        "sourceType": "module"
    },
    "parser": "babel-eslint"
}